<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.contraslash.android.pabloadventurequest">

    <application
        Mca
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".applications.game.activities.MainMenu"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".applications.authentication.activities.Splash"
            android:screenOrientation="portrait"/>
        <activity android:name=".applications.game.activities.Standard"
            android:screenOrientation="portrait"/>
        <activity android:name=".applications.game.activities.Survival"
            android:screenOrientation="portrait"/>
        <activity android:name=".applications.game.activities.Time"
            android:screenOrientation="portrait"/>
        <activity android:name=".applications.game.activities.Instructions"
            android:screenOrientation="portrait"/>
        <activity android:name=".applications.game.activities.Credits"
            android:screenOrientation="portrait"/>
        <activity android:name=".applications.game.activities.Settings"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
