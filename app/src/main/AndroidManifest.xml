<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.contraslash.android.pabloadventurequest">

    <application
        android:name=".conf.PabloAdventureQuest"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".applications.game.activities.MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".applications.authentication.activities.Splash" />
        <activity android:name=".applications.game.activities.Standard" />
        <activity android:name=".applications.game.activities.Survival" />
        <activity android:name=".applications.game.activities.Time" />
        <activity android:name=".applications.game.activities.Instructions" />
        <activity android:name=".applications.game.activities.Credits" />
        <activity android:name=".applications.game.activities.Settings"></activity>
    </application>

</manifest>
